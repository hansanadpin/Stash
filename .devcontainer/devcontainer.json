{
	"name": "Existing Docker Compose (Extend)",
	"dockerComposeFile": "../docker-compose.yml",
	"service": "stash",
	"workspaceFolder": "/app",
	"customizations": {
		"vscode": {
		  "extensions": [	
		  "dbaeumer.vscode-eslint",
          "ms-vscode.vscode-typescript",
          "ms-vscode.nodejs-extensions",
          "formulahendry.vscode-code-spell-checker", 
          "streetsidesoftware.codetime",             
          "akamikes.prettier-vscode"              
		  ],
		  "settings": {
			"terminal.integrated.shell.linux": "/bin/bash" // Example setting
		  }
		}
	  },
	  "postCreateCommand": "npm install" // (optional) 
}
